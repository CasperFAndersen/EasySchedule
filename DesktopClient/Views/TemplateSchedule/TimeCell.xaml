<UserControl x:Class="DesktopClient.TimeCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DesktopClient"
             mc:Ignorable="d" 
             AllowDrop="True"
             d:DesignHeight="300" d:DesignWidth="300"   >

    <Border Name="Border"  BorderBrush="Black" BorderThickness="0.1" >
        <Grid Name="TimeCellGrid" Margin="0,0,0,-1" Drop="OnHandleDrop" DragEnter="Border_DragEnter" DragLeave="Border_DragLeave" >
            <Grid.Background>
                <SolidColorBrush Color="White" Opacity="0"/>
            </Grid.Background>


        </Grid>
    </Border>


</UserControl>
